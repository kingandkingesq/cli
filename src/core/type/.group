#!/usr/bin/env CLI_NAME=cli bash-cli-part

help() {
    cat << EOF
Command
    ${CLI_COMMAND[@]}
EOF
}

cli::core::type::help() {
    cat
    cat << EOF

Arguments
    First argument is the type under investigation.
EOF
}

cli::core::type::import() {
    cli::meta::allow_positional
}

cli::core::type::_group::main() {
    :
    # cli::export cli core type
}

self_test() {
    cli core type is-builtin --self-test
    cli core type is-modified --self-test
    cli core type is-scaler --self-test
    cli core type is-user-defined --self-test
    cli core type unmodified --self-test
    cli core type unmodify --self-test
}