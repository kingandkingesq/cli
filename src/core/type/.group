#!/usr/bin/env CLI_NAME=cli bash-cli

help() {
    cat << EOF | cli::help::global
Command
    ${CLI_COMMAND[@]}
EOF
}

cli::core::type::help() {
    cat
    cat << EOF

Arguments
    First argument is the type under investigation.
EOF
    cli::help::global   
}

cli::core::type::import() {
    cli::meta::allow_positional
}

main() {
    cli::export cli core type
}
