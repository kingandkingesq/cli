#!/usr/bin/env CLI_NAME=cli bash-cli

help() {
    cat << EOF | cli::help::global
Command
    ${CLI_COMMAND[@]}

Arguments
    --cli -c     [Required] : Name of the cli.
EOF
}

main() (
    ${arg_cli} '---print' \
        | emit::file \
        | emit::subshell "$(cli::emit::library_name ${arg_cli})"; \
)

cli::main "$@"