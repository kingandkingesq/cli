#!/usr/bin/env CLI_NAME=cli bash-cli

help() {
    cat << EOF | cli::help::global
Command
    ${CLI_COMMAND[@]}
EOF
}

cli::emit::section() {
    echo \#
    echo \# $1
    echo \#
    while read -r; do echo "${REPLY}"; done
    echo
}

main() {
    :
    cli::export::import cli_emit
    cli::export cli emit
}

cli::main "$@"