#!/usr/bin/env bash
source $(cli loader)
cli::import_parent_group

help() {
    cat << EOF | cli::help::global
Command
    ${CLI_COMMAND}
EOF
}

cli::emit::section() {
    echo \#
    echo \# $1
    echo \#
    while read -r; do echo "${REPLY}"; done
    echo
}

export() {
    cli::export::import_parent_group
    cli::export::import cli_emit
    cli::export cli emit
}

cli::main "$@"