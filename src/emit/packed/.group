#!/usr/bin/env bash
source $(cli loader)

help() {
    cat << EOF
Command
    ${CLI_PATH}

Global Arguments
    --help -h           [Flag] : Show this message and exit.
    --self-test         [Flag] : Runs a self test.
EOF
}

cli::emit::library_name() {
    local name=$*
    echo "${name// /::}"
}

export() {
    cli::export::import cli_emit
    cli::export cli emit
}

cli::load "$@"