    cli::bash::types() {
        local -n REPLY_REF=$1
        REPLY_REF=()

        local names=( 
            $(for i in {a..z} {A..Z} _; do 
                eval "printf '%s ' \${!$i*}"
            done) 
        )

        for name in ${names[@]}; do
            declare -p ${name} \
                | read _ type name_value

            local name="${name_value%%=*}"
            REPLY_REF[${name}]=${type}
        done
    }
