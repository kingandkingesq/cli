#!/usr/bin/env CLI_NAME=cli bash-cli-part

help() {
    cat << EOF
Command
    ${CLI_COMMAND[@]}
    
Summary
    Test.
EOF
}

cli::meta::declare() {
    cli::meta::allow_positional
}

main() {
    echo "CLI_COMMAND=${CLI_COMMAND-}, \$0=$0, args=$@"
    declare -p CLI_LOADER_IMPORT CLI_LOADER_SOURCED CLI_LOADER_SOURCED_PATHS CLI_TYPE
    # cli::assert 'WE DID IT!'
}