#!/usr/bin/env CLI_NAME=cli bash-cli-part

help() {
    cat << EOF
Command
    ${CLI_COMMAND[@]}
EOF
}

main() {
    : # special loader case; export nothing
}

self_test() {
    cli path dir --self-test
    cli path name --self-test
    cli path make-absolute --self-test
    cli path make-relative --self-test
}
