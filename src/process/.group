#!/usr/bin/env CLI_NAME=cli bash-cli-part

cli::process::help() {
    cat << EOF
Command
    ${CLI_COMMAND[@]}
EOF
}

cli::process::main() {
    : # special loader case; export nothing
}

cli::process::self_test() {
    cli process signal --self-test
    cli process group --self-test
    echo cli process parent --self-test
    echo cli process tree --self-test
}
